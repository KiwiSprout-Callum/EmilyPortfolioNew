<section id="services" class="page-layout-constrained my-12 overflow-x-hidden ">
  <div class="px-6 md:px-12 lg:px-[100px] 2xl:px-[148px]">
    <h2 class="title-m mb-10 md:mb-[92px] text-center">Here's how I can help:</h2>

    <!-- Desktop Grid (hidden on mobile) -->
    <div class="hidden md:grid grid-cols-4 gap-2  md:gap-4 2xl:gap-6 max-w-fit mx-auto items-start mb-[50px]">
      {howICanHelp.map((item) => (
        <div
          class="flex flex-col px-3 py-12 rounded-xl text-center h-full justify-between"
          style={`background-color: ${item.bgColor}; color: ${item.color};`}
        >
          <div>
            <p class="mb-6 subtitle-s">{item.title}</p>
            <p class="body-alt mb-6">{item.description}</p>
          </div>
          <ul class="grid grid-cols-1  gap-4">
            {item.list.map((listItem) => (
              <li class="flex flex-col justify-between text-start items-start p-3 lg:p-4 2xl:p-8 bg-white rounded-xl min-h-[224px]">
                {listItem.icon? <div set:html={listItem.icon} />: null}
                <div class="flex flex-col gap-2">
                  <h3 class="title-xs">{listItem.title}</h3>
                  <p class="body-small-alt">{listItem.description}</p>
                </div>
              </li>
            ))}
          </ul>
        </div>
      ))}
    </div>
  
    <!-- Mobile Carousel -->
    <div
    id="carousel-wrapper"
    class="md:hidden overflow-x-auto scrollbar-hidden snap-x snap-mandatory flex scroll-smooth gap-4 px-4 mb-6"
  >
      {howICanHelp.map((item) => (
       <div
      class="snap-center shrink-0 w-full px-3 py-12 mr-4 last:mr-0 rounded-xl text-center h-full flex flex-col justify-between"
      style={`background-color: ${item.bgColor}; color: ${item.color};`}
    >
          <div>
            <p class="mb-6 subtitle-s">{item.title}</p>
            <p class="body-alt mb-6">{item.description}</p>
          </div>
          <ul class="grid grid-cols-1 gap-4">
            {item.list.map((listItem) => (
              <li class="flex md:flex-col gap-3 md:justify-between text-start items-start p-6 bg-white rounded-xl md:min-h-[294px]">
                {listItem.icon? <div set:html={listItem.icon} />: null}
                <div class="flex flex-col gap-2">
                  <h3 class="title-xs">{listItem.title}</h3>
                  <p class="body-small-alt">{listItem.description}</p>
                </div>
              </li>
            ))}
          </ul>
        </div>
      ))}
    </div>
  
    <!-- Dot Indicators -->
    <div id="carousel-dots" class="md:hidden flex justify-center gap-2 mb-8"></div>
  
    <div class="flex flex-col gap-14 md:flex-row justify-between rounded-xl bg-white items-center p-8 md:p-12">
      <p class="subtitle-s md:text-[24px] text-[32px]">Nowâ€™s the part where you tell me what youâ€™re scheming.</p>
      <div class="flex w-full justify-end items-end md:max-w-fit"><Button text="Get in touch" isFullWidth={false} href="#contact"/></div>
    </div>
  </div>
 
</section>
<script is:inline>
  const carousel = document.getElementById("carousel-wrapper");
  const dotsContainer = document.getElementById("carousel-dots");

  const slides = Array.from(carousel.children);
  const slideCount = slides.length;

  // Create dots
  slides.forEach((_, i) => {
    const dot = document.createElement("button");
    dot.className =
      "w-2 h-2 rounded-full bg-gray-300 transition-colors aria-selected:bg-black";
    dot.setAttribute("aria-label", `Slide ${i + 1}`);
    if (i === 0) dot.setAttribute("aria-selected", "true");
    dotsContainer.appendChild(dot);
  });

  const dots = Array.from(dotsContainer.children);

  // Track scroll & update active dot
  carousel.addEventListener("scroll", () => {
    const index = Math.round(carousel.scrollLeft / carousel.offsetWidth);

    dots.forEach((dot, i) => {
      if (i === index) {
        dot.setAttribute("aria-selected", "true");
      } else {
        dot.removeAttribute("aria-selected");
      }
    });
  });

  // Dot click â†’ scroll to slide
  dots.forEach((dot, i) => {
    dot.addEventListener("click", () => {
      const slideWidth = carousel.offsetWidth;
      carousel.scrollTo({
        left: i * slideWidth,
        behavior: "smooth",
      });
    });
  });

  // Optional: fix iOS scroll bounce / gesture oddities
  carousel.style.touchAction = "pan-x";
</script>